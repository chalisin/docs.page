{
  "private": true,
  "version": "0.0.0",
  "name": "docs.page",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "dev": "PORT=3001 concurrently \"npm run dev:css\" \"node -r dotenv/config node_modules/.bin/remix dev\"",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@docs.page/client": "^1.0.0",
    "@docs.page/server": "^1.0.0",
    "@docsearch/react": "^3.0.0-alpha.42",
    "@heroicons/react": "^1.0.5",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@remix-run/vercel": "^1.0.6",
    "color": "^4.1.0",
    "dotenv": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@tailwindcss/typography": "^0.5.0-alpha.3",
    "@types/color": "^3.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "classnames": "^2.3.1",
    "concurrently": "^6.4.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
